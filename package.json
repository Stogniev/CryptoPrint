{
  "name": "cryptoprint-client",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "bitcore-lib": "^0.15.0",
    "cheerio": "^1.0.0-rc.2",
    "chrome-headless-render-pdf": "github:shimondoodkin/chrome-headless-render-pdf#master",
    "firebase": "^4.2.0",
    "get-random-values": "^1.2.0",
    "jsbarcode": "^3.8.0",
    "keen-tracking": "^1.1.4",
    "localforage": "^1.5.0",
    "marked": "^0.3.6",
    "material-ui": "^0.19.4",
    "moment": "^2.18.1",
    "node-sass": "^4.7.2",
    "numeral": "^2.0.6",
    "rc-progress": "^2.2.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.2.0",
    "react-highcharts": "^11.0.0",
    "react-md": "^1.2.8",
    "react-redux": "^5.0.6",
    "react-redux-firebase": "^2.0.0-beta.17",
    "react-router": "^3.0.0",
    "react-scripts": "1.0.11",
    "react-scroll": "^1.7.3",
    "react-slick": "^0.15.4",
    "reactour": "^1.4.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-persist": "^4.8.3",
    "redux-thunk": "^2.2.0",
    "slick-carousel": "^1.8.1",
    "sync-exec": "^0.6.2",
    "webfontloader": "^1.6.28",
    "xmldom": "^0.1.27"
  },
  "scripts": {
    "build-css": "cross-env NODE_PATH=src/modules/ node-sass src/ -o src/",
    "watch-css": "cross-env NODE_PATH=src/modules/ npm run build-css && cross-env node-sass src/ -o src/ --watch --recursive",
    "start-js": "cross-env NODE_PATH=src/modules/ react-scripts start",
    "start": "cross-env node prebuild.js && cross-env NODE_PATH=src/modules/ npm-run-all -p watch-css start-js",
    "build": "cross-env node prebuild.js && cross-env NODE_PATH=src/modules/ npm run build-css && react-scripts build",
    "test": "cross-env NODE_PATH=src/modules/ react-scripts test --env=jsdom",
    "eject": "cross-env react-scripts eject",
    "print": "cross-env NODE_PATH=src/modules/ babel-node src/modules/printer --presets es2015,stage-2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "npm-run-all": "^4.1.2",
    "standard": "^10.0.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "globals": {
      "fetch": true
    }
  }
}
